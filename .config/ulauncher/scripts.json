[
    {
        "name": "rand",
        "description": "Generate a password",
        "icon": null,
        "script": "ul-t password",
        "default_arguments": [
            "32",
            "strong"
        ]
    },
    {
        "name": "start",
        "description": "Run start script",
        "icon": null,
        "script": "ul-t start",
        "default_arguments": []
    },
    {
        "name": "bar",
        "description": "Reload the conky top bar",
        "icon": null,
        "script": "conky-run panel",
        "default_arguments": []
    },
    {
        "name": "clock",
        "description": "Toggle the world clock widget",
        "icon": null,
        "script": "conky-run clock --toggle",
        "default_arguments": []
    },
    {
        "name": "monitor",
        "description": "Toggle the monitor widget",
        "icon": null,
        "script": "conky-run monitor --toggle",
        "default_arguments": []
    },
    {
        "name": "widgets",
        "description": "Toggle all conky widgets",
        "icon": null,
        "script": "conky-run clock --toggle && conky-run monitor --toggle",
        "default_arguments": []
    },
    {
        "name": "brightness",
        "description": "Fix brightness on external display",
        "icon": null,
        "script": "xrandr --output DP-1-3 --brightness 1",
        "default_arguments": []
    },
    {
        "name": "edit",
        "description": "Select to edit scripts",
        "icon": null,
        "script": "xdg-open ~/.config/ulauncher/scripts.json",
        "default_arguments": []
    },
    {
        "name": "edit-wm",
        "description": "Select to edit window manager config",
        "icon": null,
        "script": "xdg-open ~/thoams/wm-config.yml",
        "default_arguments": []
    }
]